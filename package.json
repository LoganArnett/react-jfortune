{
  "name": "react-jfortune",
  "version": "1.0.2",
  "description": "the jquery jfortune ported to react",
  "main": "dist/jfortune.bundle.js",
  "directories": {},
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "mocha --require mocha.setup.js --compilers js:babel-core/register src/*.test.js",
    "test:watch": "mocha --require mocha.setup.js --compilers js:babel-core/register src/*.test.js -w",
    "compile": "babel -d lib/ src/",
    "build": "webpack"
  },
  "keywords": [
    "jfortune",
    "react",
    "es2015",
    "mocha",
    "expect"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^2.7.1",
    "expect": "^1.20.2",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "bezier": "^0.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
